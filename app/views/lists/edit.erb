<form class="edit-list-form" action="/lists/<%= @list.id %>" method="post">
  <input type="hidden" name="_method" value="put">

  <label for="">name</label>
  <input type="text" name="list[name]" value="<%= @list.try(:name) %>">

  <label for="">description</label>
  <input type="text" name="list[description]" value="<%= @list.try(:description) %>">

  <input type="submit" value="Update!!!!!">
</form>